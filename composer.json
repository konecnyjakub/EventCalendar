{
    "name": "konecnyjakub/event-calendar",
    "description": "Plugin for Nette based web applications for creating calendar with custom events",
    "homepage": "http://www.nimral.cz/calendar",
    "license": ["BSD-3-Clause"],
    "authors": [
    {
        "name": "Jaroslav Kubíček",
        "homepage": "http://www.nimral.cz"
    }
    ],
    "require": {
        "php": ">= 5.3.0",
        "nette/utils": "~2.3",
        "latte/latte": "~2.3",
        "nette/application": "~2.3",
        "nette/neon": "~2.3"
    },
    "require-dev": {
        "nette/tester": "@dev",
        "nette/bootstrap": "~2.4",
        "nette/robot-loader": "~2.4",
        "tracy/tracy": "~2.4"
    },
    "suggest": {
        "texy/texy": ">= v2.2.0"
    },
    "autoload": {
        "psr-0": {
            "EventCalendar\\": "/"
        }
    },
    "replace": {
        "jaroslav-kubicek/event-calendar": "~0.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
