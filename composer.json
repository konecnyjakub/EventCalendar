{
    "name": "nexendrie/event-calendar",
    "description": "Plugin for Nette based web applications for creating calendar with custom events",
    "homepage": "http://www.nimral.cz/calendar",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jaroslav Kubíček",
            "homepage": "http://www.nimral.cz"
        },
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-calendar": "*",
        "nette/utils": "^3.0",
        "latte/latte": "^2.5.3 || ^3.0",
        "nette/application": "^3.0",
        "nette/neon": "^3.0"
    },
    "require-dev": {
        "nexendrie/code-quality": "@dev",
        "phpstan/phpstan-nette": "^0.12",
        "texy/texy": "^3.0.1",
        "nette/caching": "^3.0",
        "nexendrie/translation": "^2.3"
    },
    "suggest": {
        "texy/texy": "To use Texy! syntax in events",
        "nexendrie/translation": "Translator"
    },
    "conflict": {
        "texy/texy": "<3.0.1",
        "nette/caching": "<3.0"
    },
    "autoload": {
        "psr-4": {
            "EventCalendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventCalendar\\": "tests/EventCalendar"
        }
    },
    "replace": {
        "jaroslav-kubicek/event-calendar": ">=0.2",
        "konecnyjakub/event-calendar": ">=0.6.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
